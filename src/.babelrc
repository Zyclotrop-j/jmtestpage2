{
  "presets": [
    "babel-preset-gatsby"
  ],
  "plugins": [
    "transform-custom-element-classes",
    "babel-plugin-macros",
    "babel-plugin-styled-components",
    "polished",
    ["ramda", {
      "useES": true
    }],
    ["@babel/plugin-proposal-pipeline-operator", { "proposal": "smart" }],
    "@babel/plugin-proposal-function-bind",
    "@babel/plugin-proposal-optional-chaining"
  ]
}
