{
  "id": "School.json",
  "title": "school",
  "format": "http://schema.org/School",
  "media": {
    "type": "application/json;profile=http://schema.org/School"
  },
  "properties": {
    "alumni": {
      "type": "string",
      "items": {
        "type": "string",
        "x-$ref": "person"
      }
    },
    "address": {
      "type": "string",
      "x-$ref": "postaladdress"
    },
    "aggregateRating": {
      "type": "string",
      "x-$ref": "aggregaterating"
    },
    "brand": {
      "title": "Brand",
      "description": "The brand(s) associated with a product or service, or the brand(s) maintained by an organization or business person.",
      "anyOf": [
        {
          "type": "string",
          "x-$ref": "organization"
        },
        {
          "type": "string",
          "x-$ref": "brand"
        }
      ]
    },
    "contactPoint": {
      "type": "string",
      "items": {
        "type": "string",
        "x-$ref": "contactpoint"
      }
    },
    "department": {
      "type": "string",
      "items": {
        "type": "string",
        "x-$ref": "organization"
      }
    },
    "duns": {
      "title": "Duns",
      "description": "The Dun & Bradstreet DUNS number for identifying an organization or business person.",
      "type": "string"
    },
    "email": {
      "type": "array",
      "items": {
        "title": "Email",
        "description": "Email address.",
        "type": "string"
      }
    },
    "employee": {
      "type": "string",
      "items": {
        "type": "string",
        "x-$ref": "person"
      }
    },
    "event": {
      "type": "string",
      "items": {
        "type": "string",
        "x-$ref": "event"
      }
    },
    "faxNumber": {
      "title": "Fax Number",
      "description": "The fax number.",
      "type": "string"
    },
    "founder": {
      "type": "string",
      "items": {
        "type": "string",
        "x-$ref": "person"
      }
    },
    "foundingDate": {
      "title": "Founding Date",
      "description": "The date that this organization was founded.",
      "type": "string",
      "format": "date"
    },
    "globalLocationNumber": {
      "title": "Global Location Number",
      "description": "The Global Location Number (GLN, sometimes also referred to as International Location Number or ILN) of the respective organization, person, or place. The GLN is a 13-digit number used to identify parties and physical locations.",
      "type": "string"
    },
    "hasPOS": {
      "type": "string",
      "items": {
        "type": "string",
        "x-$ref": "place"
      }
    },
    "interactionCount": {
      "type": "array",
      "items": {
        "title": "Interaction Count",
        "description": "A count of a specific user interactions with this itemâ€”for example, 20 UserLikes, 5 UserComments, or 300 UserDownloads. The user interaction type should be one of the sub types of UserInteraction.",
        "type": "string"
      }
    },
    "isicV4": {
      "title": "Isic V4",
      "description": "The International Standard of Industrial Classification of All Economic Activities (ISIC), Revision 4 code for a particular organization, business person, or place.",
      "type": "string"
    },
    "legalName": {
      "title": "Legal Name",
      "description": "The official name of the organization, e.g. the registered company name.",
      "type": "string"
    },
    "location": {
      "title": "Location",
      "description": "The location of the event, organization or action.",
      "anyOf": [
        {
          "type": "string",
          "x-$ref": "postaladdress"
        },
        {
          "type": "string",
          "x-$ref": "place"
        }
      ]
    },
    "logo": {
      "type": "array",
      "items": {
        "title": "Logo",
        "description": "A logo associated with an organization.",
        "anyOf": [
          {
            "type": "string",
            "format": "uri"
          },
          {
            "type": "string",
            "x-$ref": "imageobject"
          }
        ]
      }
    },
    "makesOffer": {
      "type": "string",
      "items": {
        "type": "string",
        "x-$ref": "offer"
      }
    },
    "member": {
      "type": "array",
      "items": {
        "title": "Member",
        "description": "A member of this organization.",
        "anyOf": [
          {
            "type": "string",
            "x-$ref": "organization"
          },
          {
            "type": "string",
            "x-$ref": "person"
          }
        ]
      }
    },
    "naics": {
      "title": "Naics",
