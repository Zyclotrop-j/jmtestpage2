{
  "id": "Brand.json",
  "title": "brand",
  "format": "http://schema.org/Brand",
  "media": {
    "type": "application/json;profile=http://schema.org/Brand"
  },
  "type": "object",
  "properties": {
    "additionalType": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uri",
        "links": [
          {
            "rel": "http://schema.org/additionalType",
            "href": "{+$}",
            "linkSource": 2
          }
        ]
      }
    },
    "alternateName": {
      "type": "array",
      "items": {
        "title": "Alternate Name",
        "description": "An alias for the item.",
        "type": "string"
      }
    },
    "description": {
      "type": "array",
      "items": {
        "title": "Description",
        "description": "A short description of the item.",
        "type": "string"
      }
    },
    "image": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uri",
        "links": [
          {
            "rel": "http://schema.org/image",
            "href": "{+$}",
            "linkSource": 2
          }
        ]
      }
    },
    "name": {
      "title": "Name",
      "description": "The name of the item.",
      "type": "string"
    },
    "sameAs": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uri",
        "links": [
          {
            "rel": "http://schema.org/sameAs",
            "href": "{+$}",
            "linkSource": 2
          }
        ]
      }
    },
    "url": {
      "type": "string",
      "format": "uri"
    },
    "logo": {
      "type": "array",
      "items": {
        "title": "Logo",
        "description": "A logo associated with an organization.",
        "anyOf": [
          {
            "type": "string",
            "format": "uri"
          },
          {
            "type": "string",
            "x-$ref": "imageobject"
          }
        ]
      }
    }
  }
}