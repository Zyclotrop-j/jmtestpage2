{
  "id": "Product.json",
  "title": "product",
  "format": "http://schema.org/Product",
  "media": {
    "type": "application/json;profile=http://schema.org/Product"
  },
  "type": "object",
  "properties": {
    "additionalType": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uri",
        "links": [
          {
            "rel": "http://schema.org/additionalType",
            "href": "{+$}",
            "linkSource": 2
          }
        ]
      }
    },
    "alternateName": {
      "type": "array",
      "items": {
        "title": "Alternate Name",
        "description": "An alias for the item.",
        "type": "string"
      }
    },
    "description": {
      "type": "array",
      "items": {
        "title": "Description",
        "description": "A short description of the item.",
        "type": "string"
      }
    },
    "image": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uri",
        "links": [
          {
            "rel": "http://schema.org/image",
            "href": "{+$}",
            "linkSource": 2
          }
        ]
      }
    },
    "name": {
      "title": "Name",
      "description": "The name of the item.",
      "type": "string"
    },
    "sameAs": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uri",
        "links": [
          {
            "rel": "http://schema.org/sameAs",
            "href": "{+$}",
            "linkSource": 2
          }
        ]
      }
    },
    "url": {
      "type": "string",
      "format": "uri"
    },
    "aggregateRating": {
      "type": "string",
      "x-$ref": "aggregaterating"
    },
    "audience": {
      "type": "string",
      "x-$ref": "audience"
    },
    "brand": {
      "title": "Brand",
      "description": "The brand(s) associated with a product or service, or the brand(s) maintained by an organization or business person.",
      "anyOf": [
        {
          "type": "string",
          "x-$ref": "organization"
        },
        {
          "type": "string",
          "x-$ref": "brand"
        }
      ]
    },
    "color": {
      "title": "Color",
      "description": "The color of the product.",
      "type": "string"
    },
    "depth": {
      "title": "Depth",
      "description": "The depth of the product.",
      "anyOf": [
        {
          "type": "string",
          "x-$ref": "distance"
        },
        {
          "type": "string",
          "x-$ref": "quantitativevalue"
        }
      ]
    },
    "gtin13": {
      "title": "Gtin13",
      "description": "The GTIN-13 code of the product, or the product to which the offer refers. This is equivalent to 13-digit ISBN codes and EAN UCC-13. Former 12-digit UPC codes can be converted into a GTIN-13 code by simply adding a preceeding zero.",
      "type": "string"
    },
    "gtin14": {
      "title": "Gtin14",
      "description": "The GTIN-14 code of the product, or the product to which the offer refers.",
      "type": "string"
    },
    "gtin8": {
      "title": "Gtin8",
      "description": "The GTIN-8 code of the product, or the product to which the offer refers. This code is also known as EAN/UCC-8 or 8-digit EAN.",
      "type": "string"
    },
    "height": {
      "title": "Height",
      "description": "The height of the item.",
      "anyOf": [
        {
          "type": "string",
          "x-$ref": "distance"
        },
        {
          "type": "string",
          "x-$ref": "quantitativevalue"
        }
      ]
    },
    "isAccessoryOrSparePartFor": {
      "type": "string",
      "items": {
        "type": "string",
        "x-$ref": "product"
      }
    },
    "isConsumableFor": {
      "type": "string",
      "items": {
        "type": "string",
        "x-$ref": "product"
      }
    },
    "isRelatedTo": {
      "type": "string",
      "items": {
        "type": "string",
        "x-$ref": "product"
      }
    },
    "isSimilarTo": {
      "type": "string",
      "items": {
        "type": "string",
        "x-$ref": "product"
      }
    },
    "itemCondition": {
      "type": "string",
      "items": {
        "type": "string",
        "x-$ref": "offeritemcondition"
      }
    },
    "logo": {
      "type": "array",
      "items": {
        "title": "Logo",
        "description": "A logo associated with an organization.",
        "anyOf": [
          {
            "type": "string",
            "format": "uri"
          },
          {
            "type": "string",
            "x-$ref": "imageobject"
          }
        ]
      }
    },
    "manufacturer": {
      "type": "string",
      "x-$ref": "organization"
    },
    "model": {
      "title": "Model",
      "description": "The model of the product. Use with the URL of a ProductModel or a textual representation of the model identifier. The URL of the ProductModel can be from an external source. It is recommended to additionally provide strong product identifiers via the gtin8/gtin13/gtin14 and mpn properties.",
      "anyOf": [
        {
          "type": "string",
          "x-$ref": "productmodel"
        },
        {
          "type": "string"
        }
      ]
    },
    "mpn": {
      "title": "Mpn",
      "description": "The Manufacturer Part Number (MPN) of the product, or the product to which the offer refers.",
      "type": "string"
    },
    "offers": {
      "type": "string",
      "items": {
        "type": "string",
        "x-$ref": "offer"
      }
    },
    "productID": {
      "title": "Product ID",
      "description": "The product identifier, such as ISBN. For example: <meta itemprop='productID' content='isbn:123-456-789'/>.",
      "type": "string"
    },
    "releaseDate": {
      "title": "Release Date",
      "description": "The release date of a product or product model. This can be used to distinguish the exact variant of a product.",
      "type": "string",
      "format": "date"
    },
    "review": {
      "type": "string",
      "items": {
        "type": "string",
        "x-$ref": "review"
      }
    },
    "sku": {
      "title": "Sku",
      "description": "The Stock Keeping Unit (SKU), i.e. a merchant-specific identifier for a product or service, or the product to which the offer refers.",
      "type": "string"
    },
    "weight": {
      "type": "string",
      "x-$ref": "quantitativevalue"
    },
    "width": {
      "title": "Width",
      "description": "The width of the item.",
      "anyOf": [
        {
          "type": "string",
          "x-$ref": "distance"
        },
        {
          "type": "string",
          "x-$ref": "quantitativevalue"
        }
      ]
    }
  }
}