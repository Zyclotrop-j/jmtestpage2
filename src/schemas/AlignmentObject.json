{
  "id": "AlignmentObject.json",
  "title": "alignmentobject",
  "format": "http://schema.org/AlignmentObject",
  "media": {
    "type": "application/json;profile=http://schema.org/AlignmentObject"
  },
  "type": "object",
  "properties": {
    "additionalType": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uri",
        "links": [
          {
            "rel": "http://schema.org/additionalType",
            "href": "{+$}",
            "linkSource": 2
          }
        ]
      }
    },
    "alternateName": {
      "type": "array",
      "items": {
        "title": "Alternate Name",
        "description": "An alias for the item.",
        "type": "string"
      }
    },
    "description": {
      "type": "array",
      "items": {
        "title": "Description",
        "description": "A short description of the item.",
        "type": "string"
      }
    },
    "image": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uri",
        "links": [
          {
            "rel": "http://schema.org/image",
            "href": "{+$}",
            "linkSource": 2
          }
        ]
      }
    },
    "name": {
      "title": "Name",
      "description": "The name of the item.",
      "type": "string"
    },
    "sameAs": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uri",
        "links": [
          {
            "rel": "http://schema.org/sameAs",
            "href": "{+$}",
            "linkSource": 2
          }
        ]
      }
    },
    "url": {
      "type": "string",
      "format": "uri"
    },
    "alignmentType": {
      "type": "array",
      "items": {
        "title": "Alignment Type",
        "description": "A category of alignment between the learning resource and the framework node. Recommended values include: 'assesses', 'teaches', 'requires', 'textComplexity', 'readingLevel', 'educationalSubject', and 'educationLevel'.",
        "type": "string"
      }
    },
    "educationalFramework": {
      "title": "Educational Framework",
      "description": "The framework to which the resource being described is aligned.",
      "type": "string"
    },
    "targetDescription": {
      "title": "Target Description",
      "description": "The description of a node in an established educational framework.",
      "type": "string"
    },
    "targetName": {
      "title": "Target Name",
      "description": "The name of a node in an established educational framework.",
      "type": "string"
    },
    "targetUrl": {
      "type": "string",
      "format": "uri"
    }
  },
  "links": [
    {
      "rel": "http://schema.org/targetUrl",
      "href": "{+targetUrl}"
    }
  ]
}